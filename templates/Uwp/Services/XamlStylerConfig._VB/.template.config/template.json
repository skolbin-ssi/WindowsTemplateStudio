{
  "$schema": "http://json.schemastore.org/template",
  "author": "Microsoft Community",
  "classifications": ["Universal"],
  "name": "XAML Styler Config",
  "shortName": "XAML Styler Config",
  "groupIdentity": "wts.Service.XAMLStyler.VB",
  "identity": "wts.Service.XAMLStyler.VB",
  "description": "Default XAML Styler config file.",
  "tags": {
    "language": "VisualBasic",
    "type": "item",
    "wts.type": "service",
    "wts.projecttype": "all",
    "wts.frontendframework": "MVVMBasic|MVVMLight|CodeBehind|MVVMToolkit",
    "wts.platform": "Uwp",
    "wts.group": "Tools",
    "wts.version": "1.0.0",
    "wts.displayOrder": "1",
    "wts.defaultInstance": "XAMLStyler",
    "wts.multipleInstance": "false",
    "wts.isHidden": "false",
    "wts.genGroup": "0",
    "wts.rightClickEnabled":"true",
    "wts.outputToParent": "true",
    "wts.licenses": "[XAMLStyler](https://marketplace.visualstudio.com/items/TeamXavalon.XAMLStyler/license)"
  },
  "sourceName": "XAMLStylerFeature",
  "preferNameDirectory": true,
  "PrimaryOutputs": [
    {
      "path": "Param_ProjectName/Settings.XamlStyler"
    }
  ],
  "symbols": {
    "wts.projectName": {
      "type": "parameter",
      "replaces": "Param_ProjectName",
      "fileRename": "Param_ProjectName"
    },
    "wts.rootNamespace": {
      "type": "parameter",
      "replaces": "Param_RootNamespace"
    }
  }
}
